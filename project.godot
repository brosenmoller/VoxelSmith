; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="VoxelSmith"
config/description="Voxel Smith is a Game Development tool created in Godot. It is a voxel based level editor which allows you to export to OBJ and UnityPrefab."
config/version="0.7.0"
run/main_scene="res://Assets/Scenes/main.tscn"
config/features=PackedStringArray("4.5", "C#", "Forward Plus")
run/max_fps=200
config/icon="res://Assets/Art/Logo/logo.png"

[autoload]

GameManager="*res://Assets/Scripts/Management/GameManager.cs"

[display]

window/size/viewport_width=1872
window/size/viewport_height=1053
window/stretch/aspect="keep_height"
window/vsync/vsync_mode=0

[dotnet]

project/assembly_name="VoxelSmith"

[file_customization]

folder_colors={
"res://Assets/Art/": "blue",
"res://Assets/Resources/": "green",
"res://Assets/Scenes/": "red",
"res://Assets/Scripts/": "yellow"
}

